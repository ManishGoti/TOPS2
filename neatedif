    printf("enter the value if c:");
    scanf("%d,c");
    int a = 20, b = 45, c = 25;
    if (a > b)
    {
    if (a > c)
        {
            printf("A is Greater");
        }
        else
        {
            printf("C is greater");
        }
    }
    else
    {
        if (b > c)
        {
